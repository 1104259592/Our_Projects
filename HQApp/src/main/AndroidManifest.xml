<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hq.app">

    <application
        android:name=".mylibrary.baseconfigure.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.EventDetailActivity"></activity>
        <activity
            android:name=".activitys.LogoActivity"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activitys.MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.ThemeSettingActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activitys.MapActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>